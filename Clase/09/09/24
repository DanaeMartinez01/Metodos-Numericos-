        "# Funci칩n cuadr치tica.\n",
        "def f(x):\n",
        "    return (x+1)**(1/2)\n",
        "def g(x):\n",
        "    return(1+(1/2)*x)\n",
        "def h(x):\n",
        "    return(1+((1/2)*x)-((1/8)*(x**2)))\n",
        "def j(x):\n",
        "    return(1+((1/2)*x)-((1/8)*(x**2))+((1/16)*(x**3)))"
      ]
    },
